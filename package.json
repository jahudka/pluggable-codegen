{
  "name": "pluggable-codegen",
  "version": "0.1.1",
  "license": "MIT",
  "contributors": [
    {
      "name": "Dan Kadera",
      "email": "me@subsonic.cz",
      "url": "https://github.com/jahudka"
    }
  ],
  "files": [
    "dist"
  ],
  "bin": {
    "pluggable-codegen": "dist/cli.js"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./presets/*": {
      "types": "./dist/presets/*.d.ts",
      "require": "./dist/presets/*.cjs",
      "import": "./dist/presets/*.js"
    }
  },
  "dependencies": {
    "picomatch": "^4.0.2",
    "tinyglobby": "^0.2.13"
  },
  "peerDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "yaml": "^2.7.0"
  },
  "peerDependenciesMeta": {
    "@graphql-codegen/cli": {
      "optional": true
    },
    "yaml": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.25.1",
    "@graphql-codegen/cli": "^5.0.5",
    "@types/node": "^22.15.3",
    "@types/picomatch": "^4.0.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "rollup": "^4.40.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3",
    "yaml": "^2.7.1"
  }
}
